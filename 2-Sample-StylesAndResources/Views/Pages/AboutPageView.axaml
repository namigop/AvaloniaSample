<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:pages="clr-namespace:AvaloniaSampleTutorial.ViewModels.Pages"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="pages:AboutPageViewModel"
             x:Class="AvaloniaSampleTutorial.Views.Pages.AboutPageView">
   <Border Background="{StaticResource MyControlBackground}">
       <StackPanel Orientation="Vertical" VerticalAlignment="Center" HorizontalAlignment="Center">
           <TextBlock Text="h1: Our sample app" Classes="h1" ></TextBlock>
           <TextBlock Text="h2 (with nested style): the is a subtitle." Classes="h2" ></TextBlock>
           <TextBlock Text="this is normal text. Lorem ipsum dolor foo bar"  ></TextBlock>
       </StackPanel>
   </Border>
</UserControl>
